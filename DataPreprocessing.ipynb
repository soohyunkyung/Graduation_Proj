import os
import pandas as pd
import numpy as np
from sklearn.model_selection import train_test_split
from sklearn.preprocessing import LabelEncoder
from tensorflow.keras.preprocessing.image import ImageDataGenerator
from tensorflow.keras.callbacks import EarlyStopping, ModelCheckpoint
from tensorflow.keras.optimizers import Adam
from tensorflow.keras import layers, models
import matplotlib.pyplot as plt
import kagglehub

dogcsv=pd.read_csv("/content/dog_breeds_info.csv")
# 1. 데이터 불러오기
dog_breed_classification_path = kagglehub.dataset_download('dilakshanchandrasena/dog-breed-classification')
dataset_folder = os.path.join(dog_breed_classification_path, 'DATASET')
tr_image_folder = os.path.join(dataset_folder, 'train_images')
tr_csv_path = os.path.join(dataset_folder, 'train_data.csv')
val_image_folder = os.path.join(dataset_folder, 'test_images')
val_csv_path = os.path.join(dataset_folder, 'test_data.csv')

tr_df = pd.read_csv(tr_csv_path)
tr_df['filename'] = tr_df['id'].astype(str) + '.jpg' #파일 경로 형태의 문자열로 변경
tr_df['filepath'] = tr_df['filename'].apply(lambda x: os.path.join(tr_image_folder, x)) #문자열에서 파일 경로로 type을 변경

val_df = pd.read_csv(val_csv_path)
val_df['filename'] = val_df['id'].astype(str) + '.jpg'
val_df['filepath'] = val_df['filename'].apply(lambda x: os.path.join(val_image_folder, x))

dog_breed_photos_path = kagglehub.dataset_download('enashed/dog-breed-photos')
tr_image_folder2 = os.path.join(dog_breed_photos_path, 'dog_breed_photos', 'dog_breed_photos')
tr_csv_path2 = os.path.join(dog_breed_photos_path, 'dog_breed_photos.csv')
tr_df2 = pd.read_csv(tr_csv_path2)

# 2. 데이터 전처리
tr_df2 = tr_df2.drop(columns=['DogId', 'ImageNumber'])
tr_df2 = tr_df2.rename(columns={'Breed': 'breed', 'Image': 'filename'})
tr_df2['breed'] = tr_df2['breed'].str.lower().str.replace(' ', '', regex=False).str.replace('_', '', regex=False).str.replace('-', '', regex=False)
tr_df2['breed'] = tr_df2['breed'].str.replace('cross', '', regex=False).str.strip()
tr_df2['filepath'] = tr_df2['filename'].apply(lambda x: os.path.join(tr_image_folder2, x))

# 기존 데이터셋 전처리
tr_df['breed'] = tr_df['breed'].str.lower().str.replace(' ', '', regex=False).str.replace('_', '', regex=False).str.replace('-', '', regex=False)
val_df['breed'] = val_df['breed'].str.lower().str.replace(' ', '', regex=False).str.replace('_', '', regex=False).str.replace('-', '', regex=False)

# 3. 병합 (tr + tr2 + val 데이터 통합)
combined_df = pd.concat([tr_df[['filepath', 'breed']], val_df[['filepath', 'breed']], tr_df2[['filepath', 'breed']]], ignore_index=True)
#100장 미만 견종은 버리기
breed_counts = combined_df['breed'].value_counts()
valid_breeds = breed_counts[breed_counts > 100].index
combined_df = combined_df[combined_df['breed'].isin(valid_breeds)].reset_index(drop=True)

#dogcsv['Breed']와 같은 breed만 남기기
info_breeds = dogcsv['Breed']
allowed_breeds_from_info = set(info_breeds)
combined_df = combined_df[combined_df['breed'].isin(allowed_breeds_from_info)].reset_index(drop=True)

# 4. 라벨 인코딩
le = LabelEncoder()
combined_df['label'] = le.fit_transform(combined_df['breed'])
# breed 기준으로 필터링 (문자열 vs 문자열)
allowed_breeds = set(tr_df['breed'].unique())
combined_df = combined_df[combined_df['breed'].isin(allowed_breeds)].reset_index(drop=True)

print("Label Mapping:", dict(zip(le.classes_, le.transform(le.classes_))))



##. 변경할 품종 딕셔너리 정의
# Key: 통합 품종 이름, Value: 통합될 기존 품종 이름 리스트
breed_mapping = {
    # 푸들 계열 통일
    'poodle': ['toypoodle', 'miniaturepoodle', 'standardpoodle'],

    # 시각 하운드 계열 통일
    'hound': ['whippet', 'italiangreyhound'],

    # 리트리버 계열 통일
    'retriever': ['goldenretriever', 'labradorretriever'],

    # 목양견 계열 통일 (shetlandsheepdog를 bordercollie로 통일)
    'bordercollie': ['shetlandsheepdog']
}

#. .replace() 메서드를 위한 딕셔너리 구조 평탄화 (old_breed: new_breed)
replace_dict = {}
for new_breed, old_breeds in breed_mapping.items():
    for old_breed in old_breeds:
        replace_dict[old_breed] = new_breed

#. .replace() 메서드를 사용하여 'breed' 컬럼 값 변경
combined_df['breed'] = combined_df['breed'].replace(replace_dict)



# 5. train/test split
tr_df, val_df = train_test_split(combined_df, stratify=combined_df['label'], test_size=0.2, random_state=42)

#유효한 이미지만 남기기 + 흑백 이미지 RGB 파일로 만들기
from PIL import Image

def is_valid_image_and_convert_rgb(path):
    try:
        # 1) 존재 확인
        if not os.path.exists(path):
            return False

        # 2) 유효성 확인
        with Image.open(path) as img:
            img.verify()  # 여기서 파일핸들이 소모됨

        # 3) 다시 열어 RGB 변환/저장
        with Image.open(path) as img2:
            if img2.mode != 'RGB':
                img2 = img2.convert('RGB')
                img2.save(path)

        return True
    except Exception:
        return False

# 존재 필터 → 유효/변환 필터 순서 추천
tr_df = tr_df[tr_df['filepath'].apply(os.path.exists)].reset_index(drop=True)
val_df = val_df[val_df['filepath'].apply(os.path.exists)].reset_index(drop=True)

tr_df = tr_df[tr_df['filepath'].apply(is_valid_image_and_convert_rgb)].reset_index(drop=True)
val_df = val_df[val_df['filepath'].apply(is_valid_image_and_convert_rgb)].reset_index(drop=True)

# --- 6. 이미지 전처리 ---
from tensorflow.keras.preprocessing.image import ImageDataGenerator
import pandas as pd

IMG_SIZE = (384, 384)
BATCH_SIZE = 8

train_gen = ImageDataGenerator(
    rescale=1./255,
    rotation_range=40,
    width_shift_range=0.2,
    height_shift_range=0.2,
    shear_range=0.2,
    zoom_range=0.3,
    horizontal_flip=True,
    fill_mode='nearest'
)
val_gen = ImageDataGenerator(rescale=1./255)

# train/val 공통으로 존재하는 품종만 classes로 고정 (문자 라벨)
classes = sorted(pd.Index(tr_df['breed'].unique()).intersection(val_df['breed'].unique()).tolist())
print('num classes:', len(classes))

train_generator = train_gen.flow_from_dataframe(
    tr_df,
    x_col='filepath',
    y_col='breed',
    target_size=IMG_SIZE,
    batch_size=BATCH_SIZE,
    class_mode='categorical',
    classes=classes, shuffle=True
)

val_generator = val_gen.flow_from_dataframe(
    val_df,
    x_col='filepath',
    y_col='breed',
    target_size=IMG_SIZE,
    batch_size=BATCH_SIZE,
    class_mode='categorical',
    classes=classes, shuffle=False
)
